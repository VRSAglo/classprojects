package edu.mtc.egr283.Project01;

public class SuitDriver {

	public static void main(String[] args) {
		
		for(Suit s : Suit.values()) {
			System.out.println(s.getName() + " : " + s.getSymbol());
		}//Ending bracket of for each loop
	}//Ending bracket of main method
}//ending bracket of SuitDriver class
