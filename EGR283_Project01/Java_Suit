package edu.mtc.egr283.Project01;

public enum Suit {
	
	SPADES("Spades", '\u2660'), CLUBS("Clubs", '\u2663'), HEARTS("Hearts", '\u2665'), DIAMONDS("Diamonds", '\u2666');

	private String name;
	private char symbol;
	
	private Suit(String newName, char newSymbol) {
		this.name = newName;
		this.symbol = newSymbol;
	}//ending bracket of constructor
	
	public String getName() {
		return this.name;
	}//ending bracket of getName method.
	
	public char getSymbol() {
		return this.symbol;
	}//ending bracket of getSymbol method.
	
}//ending bracket of enum suit.
